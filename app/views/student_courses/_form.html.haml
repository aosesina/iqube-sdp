= form_for @student_course do |f|
  - if @student_course.errors.any?
    #error_explanation
      %h2= "#{pluralize(@student_course.errors.count, "error")} prohibited this student_course from being saved:"
      %ul
        - @student_course.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :student_id
    = f.number_field :student_id
  .field
    = f.label :course_id
    = f.number_field :course_id
  .actions
    = f.submit 'Save'
